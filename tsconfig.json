{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "esnext.asynciterable"
      ],
    "target":"ES6",
    "module":"ES6",
    "moduleResolution": "node",
    "esModuleInterop": false,
    "outDir": "./dist",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}